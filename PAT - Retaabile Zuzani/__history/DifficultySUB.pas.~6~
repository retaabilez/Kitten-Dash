  Vcl.Imaging.jpeg, Vcl.ExtCtrls;

type
  TfrmSub = class(TForm)
    imgBackground: TImage;
    btnSingle: TButton;
    Button1: TButton;
    Button2: TButton;
    Image1: TImage;
    btnHome: TButton;
    procedure FormCreate(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }

    bEasy : Boolean;
    bMedium : Boolean;
    bHard : Boolean;

  end;

var
  frmSub: TfrmSub;

implementation

{$R *.dfm}

